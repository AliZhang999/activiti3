        <timeDuration>PT5M</timeDuration>
      </timerEventDefinition>
    </intermediateCatchEvent>
    <sequenceFlow id="flow5" sourceRef="usertask2" targetRef="timerintermediatecatchevent1"></sequenceFlow>
    <sequenceFlow id="flow6" sourceRef="timerintermediatecatchevent1" targetRef="usertask1"></sequenceFlow>
